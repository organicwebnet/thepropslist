# iOS Firebase Configuration
# Copy this file to .env and fill in your actual values
# Get these values from your Firebase project console

# Firebase Project Configuration
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id

# iOS-specific Firebase Configuration
EXPO_PUBLIC_FIREBASE_IOS_API_KEY=your-ios-api-key
EXPO_PUBLIC_FIREBASE_IOS_APP_ID=your-ios-app-id

# Google Services Configuration
EXPO_PUBLIC_GOOGLE_API_KEY=your-google-api-key
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-web-client-id

# Other API Keys
EXPO_PUBLIC_TICKETMASTER_API_KEY=your-ticketmaster-api-key

# Path to iOS GoogleService-Info.plist file
GOOGLE_SERVICES_PLIST=./ios/GoogleService-Info.plist

